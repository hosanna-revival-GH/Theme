    },
    "text_and_image_hC948e_5": {
      "type": "text-and-image",
      "settings": {
        "image": "shopify://shop_images/5-Amanda-Devlin-Writer-Headshot.jpg",
        "image_mask": "none",
        "image2_mask": "none",
        "subtitle": "Day Five",
        "title": "Amanda Devlin",
        "text": "<p>Amanda is a professional cellist and writer living in sunny California. She has three beautiful kids and is blessed to be married to her best friend. You can find her most days chauffeuring her kids around town, meeting with friends for coffee, or visiting her favorite place: the beach. </p>",
        "button_label": "Read her WHEN Article",
        "button_link": "https://hosannarevival.blog/author/amanda-devlin/",
        "button_style": "primary",
        "align_text": "left",
        "image_width": "50",
        "layout": "left",
