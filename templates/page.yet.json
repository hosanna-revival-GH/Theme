    "text_columns_BQGmdg": {
      "type": "text-columns",
      "blocks": {
        "text_block_40": {
          "type": "text_block",
          "settings": {
            "enable_image": true,
            "image": "shopify://shop_images/Breakfast-Table-Songs-for-the-Suffering-Devo-Hosanna-Revival-P-006.jpg",
            "image_mask": "none",
            "image_width": 650,
            "title": "To the One Suffering",
            "text": "<p>If you are walking through a season of suffering right now—this is for YOU.</p>",
            "button_label": "Watch this Video",
            "button_link": "https://www.instagram.com/tv/CN2wKzMBuwy/"
          }
        },
        "text_block_41": {
          "type": "text_block",
          "settings": {
            "enable_image": true,
            "image": "shopify://shop_images/Breakfast-Table-Songs-for-the-Suffering-Devo-Hosanna-Revival-P-004.jpg",
            "image_mask": "none",
            "image_width": 650,
            "title": "To the one walking alongside the suffering",
            "text": "<p>Whether you have a friend struggling with infertility, a family member with a scary diagnosis, or a loved one struggling with anxiety, walking with the suffering is a profound work...</p>",
            "button_label": "Watch This Video",
            "button_link": "https://www.instagram.com/tv/COK_mTeB0Gq/"
