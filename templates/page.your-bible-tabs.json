        "text_PqKkhP": {
          "type": "text",
          "settings": {
            "enlarge_text": false,
            "text": "<p>Need the alignment guide? Download <a href=\"https://cdn.shopify.com/s/files/1/0074/7674/0155/files/Gold-Bible-Tabs-Mission-Card-Back.pdf?v=1741791653\" title=\"https://cdn.shopify.com/s/files/1/0074/7674/0155/files/Gold-Bible-Tabs-Mission-Card-Back.pdf?v=1741791653\"><em>here</em></a><em>!</em></p>",
            "align_text_block": "inherit",
            "block_bottom_space": 30
          }
        }
      },
      "block_order": [
        "heading_3RnJzA",
        "text_PqKkhP"
      ],
      "settings": {
        "align_text": "center",
        "narrow_column": true,
        "divider": false,
