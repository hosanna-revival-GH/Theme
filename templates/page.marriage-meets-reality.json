    "rich_text_hjcJjW": {
      "type": "rich-text",
      "blocks": {
        "heading_mqtw6M": {
          "type": "heading",
          "settings": {
            "title": "A Prayer to Protect Your Marriage"
          }
        },
        "text_4UN8Gr": {
          "type": "text",
          "settings": {
            "enlarge_text": true,
            "text": "<p><em>Lord, </em></p>",
            "align_text_block": "left",
            "block_bottom_space": 30
          }
        },
        "text_T4T7rQ": {
          "type": "text",
          "settings": {
            "enlarge_text": true,
            "text": "<h2></h2><p>Lord, thank you that our marriage is sheltered by your grace and mercy. In your enveloping love, please protect us from believing the worst of the other on the bad days and fill our hearts with gratitude on the good days. Help us be wise about the schemes of hell to disrupt and divide the true unity we have in you. </p>",
            "align_text_block": "left",
            "block_bottom_space": 30
          }
        },
        "text_Pwrgce": {
          "type": "text",
          "settings": {
            "enlarge_text": true,
            "text": "<p><em>In Jesus</em>’s<em> name, amen.</em></p>",
            "align_text_block": "left",
            "block_bottom_space": 30
          }
        }
      },
      "block_order": [
        "heading_mqtw6M",
